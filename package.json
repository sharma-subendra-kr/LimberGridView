{
  "name": "@sharma-subendra-kr/limber-grid-view",
  "version": "1.0.0-beta.12",
  "description": "LimberGridView, a powerful JavaScript Library using Computational Geometry to render movable, dynamically resizable, and auto-arranging grids.",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "start": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js && mv ./dist/umd/index.css ./dist/",
    "lint:check": "prettier -c **/*.{js,css,scss,json}; eslint \"**/*.js\"",
    "lint:fix": "prettier --write **/*.{js,css,scss,json} && eslint --fix \"**/*.js\"",
    "prettier:fix": "prettier --write **/*.{js,css,scss,json}",
    "eslint:fix": "eslint --fix \"**/*.js\"",
    "prepare": "husky install",
    "gen-doc": "rm -r ./jsdoc/* && jsdoc -c jsDocConf.json",
    "gen-markdown-doc": "jsdoc2md ./src/index.js > ./jsdocToMarkdown/docs.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sharma-subendra-kr/LimberGridView.git"
  },

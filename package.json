    "@babel/plugin-proposal-optional-chaining": "^7.14.2",
    "@babel/preset-env": "^7.14.4",
    "autoprefixer-loader": "^3.2.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "clean-terminal-webpack-plugin": "^3.0.0",
    "css-loader": "^2.1.1",
    "docdash": "^1.2.0",
